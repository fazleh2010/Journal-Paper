@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :LexicalEntry_1 ;
  lemon:entry    :of .

:LexicalEntry_1 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :LexicalEntry_1_form ;
  lemon:synBehavior    :LexicalEntry_1_nounpp ;
  lemon:sense          :LexicalEntry_1_sense_ontomap .

:LexicalEntry_1_form a lemon:Form ;
  lemon:writtenRep "scheda"@en .

:LexicalEntry_1_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:LexicalEntry_1_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :LexicalEntry_1_sense_ontomap ;
  lemon:reference           <http://purl.org/dc/elements/1.1/source> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :LexicalEntry_1_condition .

:LexicalEntry_1_condition a lemon:condition ;
  lemon:propertyDomain  <https://dati.beniculturali.it/lodview-arco/ontology/arco/HistoricOrArtisticProperty> ;
  lemon:propertyRange   <https://w3id.org/arco/ontology/catalogue/CatalogueRecordD> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "di"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .