@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :hoch ;
  lemon:entry    :form_hoch_preposition ;
  lemon:entry    :hoch_res .

:hoch a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_hoch ;
  lemon:otherForm      :form_hoch_comperative ;
  lemon:otherForm      :form_hoch_superlative ;
  lemon:sense          :hoch_sense_1 ;
  lemon:synBehavior    :hoch_predFrame .

:form_hoch lemon:writtenRep "hoch"@de .

:form_hoch_comperative lemon:writtenRep "h�her"@de .

:form_hoch_superlative lemon:writtenRep "h�chste"@de .

:hoch_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :hoch_PredSynArg .

:hoch_sense_1 a  lemon:LexicalSense ;
  lemon:reference :hoch_res ;
  lemon:isA       :hoch_PredSynArg ;
  lemon:condition :hoch_sense_1_condition .

:hoch_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/elevation> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:hoch_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/Mountain> .## Prepositions ##
:form_hoch_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

