@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :capital-of-3 ;
  lemon:entry    :preposition .

:capital-of-3 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :capital-of-3_form ;
  lemon:otherForm      :capital-of-3_plural_form ;
  lemon:otherForm      :capital-of-3_accusative_form ;
  lemon:otherForm      :capital-of-3_dative_form ;
  lemon:otherForm      :capital-of-3_genitive_form ;
  lemon:otherForm      :capital-of-3_plural_form ;
  lemon:sense          :capital-of-3_sense_1 ;
  lemon:synBehavior    :capital-of-3_nounpp .

:capital-of-3_form a lemon:Form ;
  lemon:writtenRep "Hauptstadt"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:nominativeCase .

:capital-of-3_plural_form a lemon:Form ;
  lemon:writtenRep "-"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:plural ;
  lexinfo:case     lexinfo:nominativeCase .

:capital-of-3_accusative_form a lemon:Form ;
  lemon:writtenRep "Hauptstadt"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:accusativeCase .

:capital-of-3_dative_form a lemon:Form ;
  lemon:writtenRep "Hauptstadt"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:dativeCase .

:capital-of-3_genitive_form a lemon:Form ;
  lemon:writtenRep "Hauptstadt"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:genitiveCase .

:capital-of-3_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:capital-of-3_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :capital-of-3_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/capital> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :capital-of-3_sense_1_condition .

:capital-of-3_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Location> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :preposition .

## Prepositions ##

:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .