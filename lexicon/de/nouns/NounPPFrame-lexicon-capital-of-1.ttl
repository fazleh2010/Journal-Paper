@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :capital-of-1 ;
  lemon:entry    :von .

:capital-of-1 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :capital-of-1_form ;
  lemon:sense          :capital-of-1_sense_1 ;
  lemon:synBehavior    :capital-of-1_nounpp .

:capital-of-1_form a lemon:Form ;
  lemon:writtenRep "Hauptstadt"@de .

:capital-of-1_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:capital-of-1_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :capital-of-1_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/capital> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :capital-of-1_sense_1_condition .

:capital-of-1_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :von .

## Prepositions ##

:von a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .