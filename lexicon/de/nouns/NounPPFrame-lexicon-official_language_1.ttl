@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :official_language_1 ;
  lemon:entry    :preposition .

:official_language_1 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :official_language_1_form ;
  lemon:otherForm      :official_language_1_plural_form ;
  lemon:otherForm      :official_language_1_accusative_form ;
  lemon:otherForm      :official_language_1_dative_form ;
  lemon:otherForm      :official_language_1_genitive_form ;
  lemon:otherForm      :official_language_1_plural_form ;
  lemon:sense          :official_language_1_sense_1 ;
  lemon:synBehavior    :official_language_1_nounpp .

:official_language_1_form a lemon:Form ;
  lemon:writtenRep "Amtssprache"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:nominativeCase .

:official_language_1_plural_form a lemon:Form ;
  lemon:writtenRep "Amtssprachen"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:plural ;
  lexinfo:case     lexinfo:nominativeCase .

:official_language_1_accusative_form a lemon:Form ;
  lemon:writtenRep "Amtssprache"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:accusativeCase .

:official_language_1_dative_form a lemon:Form ;
  lemon:writtenRep "Amtssprache"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:dativeCase .

:official_language_1_genitive_form a lemon:Form ;
  lemon:writtenRep "Amtssprache"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:genitiveCase .

:official_language_1_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:official_language_1_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :official_language_1_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/officialLanguage> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :official_language_1_sense_1_condition .

:official_language_1_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Language> .

:arg2 lemon:marker :preposition .

## Prepositions ##

:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .