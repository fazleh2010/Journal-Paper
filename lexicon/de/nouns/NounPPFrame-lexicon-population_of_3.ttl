@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :population_of_3 ;
  lemon:entry    :von .

:population_of_3 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :population_of_3_form ;
  lemon:otherForm      :population_of_3_plural_form ;
  lemon:otherForm      :population_of_3_accusative_form ;
  lemon:otherForm      :population_of_3_dative_form ;
  lemon:otherForm      :population_of_3_genitive_form ;
  lemon:otherForm      :population_of_3_plural_form ;
  lemon:sense          :population_of_3_sense_1 ;
  lemon:synBehavior    :population_of_3_nounpp .

:population_of_3_form a lemon:Form ;
  lemon:writtenRep "Gesamteinwohnerzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:nominativeCase .

:population_of_3_plural_form a lemon:Form ;
  lemon:writtenRep "-"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:plural ;
  lexinfo:case     lexinfo:nominativeCase .

:population_of_3_accusative_form a lemon:Form ;
  lemon:writtenRep "Gesamteinwohnerzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:accusativeCase .

:population_of_3_dative_form a lemon:Form ;
  lemon:writtenRep "Gesamteinwohnerzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:dativeCase .

:population_of_3_genitive_form a lemon:Form ;
  lemon:writtenRep "Gesamteinwohnerzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:genitiveCase .

:population_of_3_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:population_of_3_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :population_of_3_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/populationTotal> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :population_of_3_sense_1_condition .

:population_of_3_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Settlement> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :von .

## Prepositions ##

:von a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .