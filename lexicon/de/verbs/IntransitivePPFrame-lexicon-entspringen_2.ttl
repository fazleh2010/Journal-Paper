@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :entspringen_2 ;
  lemon:entry    :form_entspringen_2_present_preposition .

:entspringen_2 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_entspringen_2_present ;
  lemon:otherForm      :entspringen_2_past ;
  lemon:otherForm      :entspringen_2_perfect ;
  lemon:sense          :entspringen_2_sense_1 ;
  lemon:synBehavior    :entspringen_2_frame .

:form_entspringen_2_presententspringen_2_present a           lemon:Form ;
  lemon:writtenRep     "entspringen"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:entspringen_2_present a      lemon:Form ;
  lemon:writtenRep "entspringt"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:entspringen_2_past a lemon:Form ;
  lemon:writtenRep  "entsprang"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:entspringen_2_perfect a lemon:Form ;
  lemon:writtenRep  "entsprungen"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:entspringen_2_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :entspringen_2_subj ;
  lexinfo:prepositionalAdjunct :entspringen_2_obj .

:entspringen_2_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :entspringen_2_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/sourceCountry> ;
  lemon:subjOfProp  :entspringen_2_obj ;
  lemon:objOfProp   :entspringen_2_subj ;
  lemon:condition   :entspringen_2_sense_1_condition .

:entspringen_2_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/BodyOfWater> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:entspringen_2_obj lemon:marker :in .

:arg2 lemon:marker :form_entspringen_2_present_preposition .

## Prepositions ##

:form_entspringen_2_present_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .