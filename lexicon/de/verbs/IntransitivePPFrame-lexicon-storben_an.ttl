@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :storben_an ;
  lemon:entry    :form_storben_an_present_preposition .

:storben_an a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_storben_an_present ;
  lemon:otherForm      :storben_an_present3rd ;
  lemon:otherForm      :storben_an_past ;
  lemon:otherForm      :storben_an_perfect ;
  lemon:sense          :storben_an_sense_1 ;
  lemon:synBehavior    :storben_an_frame .

:form_storben_an_present a           lemon:Form ;
  lemon:writtenRep     "sterben"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:storben_an_present3rd a      lemon:Form ;
  lemon:writtenRep "stirbt"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:storben_an_past a lemon:Form ;
  lemon:writtenRep  "starb"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:storben_an_perfect a lemon:Form ;
  lemon:writtenRep  "gestorben"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:storben_an_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :storben_an_subj ;
  lexinfo:prepositionalAdjunct :storben_an_obj .

:storben_an_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :storben_an_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/deathCause> ;
  lemon:subjOfProp  :storben_an_obj ;
  lemon:objOfProp   :storben_an_subj ;
  lemon:condition   :storben_an_sense_1_condition .

:storben_an_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Disease> .


:storben_an_obj lemon:marker :an .

:arg2 lemon:marker :form_storben_an_present_preposition .

## Prepositions ##

:form_storben_an_present_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "an"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .