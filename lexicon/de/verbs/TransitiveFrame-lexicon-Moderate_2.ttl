@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :to_Moderate_2 ;
  lemon:entry    :Moderate_2ed ;
  lemon:entry    :preposition .

:to_Moderate_2 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_Moderate_2 ;
  lemon:otherForm      :form_Moderate_2s ;
  lemon:otherForm      :form_Moderate_2ed ;
  lemon:sense          :Moderate_2_sense_1 ;
  lemon:synBehavior    :Moderate_2_frame_transitive .

:form_Moderate_2 a         lemon:Form ;
  lemon:writtenRep     "moderieren"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_Moderate_2s a    lemon:Form ;
  lemon:writtenRep "moderiert"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .


:form_Moderate_2s a    lemon:Form ;
  lemon:writtenRep "moderiert"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .

:form_Moderate_2ed a   lemon:Form ;
  lemon:writtenRep "moderiert"@de ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:Moderate_2_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :Moderate_2_subj ;
  lexinfo:directObject     :Moderate_2_obj .

:Moderate_2_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :Moderate_2_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/presenter> ;
  lemon:subjOfProp  :Moderate_2_obj ;
  lemon:objOfProp   :Moderate_2_subj ;
  lemon:condition   :Moderate_2_sense_1_condition .


:Moderate_2_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:Moderate_2ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_Moderate_2ed_canonical ;
  lemon:otherForm      :form_Moderate_2ed_by ;
  lemon:synBehavior    :Moderate_2ed_frame_adjectivepp ;
  lemon:sense          :Moderate_2ed_ontomap .

:form_Moderate_2ed_canonical a lemon:Form ;
  lemon:writtenRep         "moderieren"@de .

:form_Moderate_2ed_by a    lemon:Form ;
  lemon:writtenRep     "moderieren"@de ;
  lexinfo:verbFormMood lexinfo:participle .


:Moderate_2ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :Moderate_2ed_subj ;
  lexinfo:prepositionalAdjunct :Moderate_2ed_obj .

:Moderate_2ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :Moderate_2ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/presenter> ;
  lemon:subjOfProp  :Moderate_2ed_subj ;
  lemon:objOfProp   :Moderate_2ed_obj ;
  lemon:condition   :Moderate_2_sense_1_condition .
:Moderate_2ed_obj lemon:marker :von .

## Prepositions ##
:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

