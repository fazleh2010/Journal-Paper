@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :complete_in ;
  lemon:entry    :form_complete_in_present_preposition .

:complete_in a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_complete_in_present ;
  lemon:otherForm      :complete_in_past ;
  lemon:otherForm      :complete_in_perfect ;
  lemon:sense          :complete_in_sense_1 ;
  lemon:synBehavior    :complete_in_frame .

:form_complete_in_presentcomplete_in_present a           lemon:Form ;
  lemon:writtenRep     "fertigstellen"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:complete_in_present a      lemon:Form ;
  lemon:writtenRep "stellt fertig"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:complete_in_past a lemon:Form ;
  lemon:writtenRep  "stellte fertig"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:complete_in_perfect a lemon:Form ;
  lemon:writtenRep  "fertiggestellt"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:complete_in_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :complete_in_subj ;
  lexinfo:prepositionalAdjunct :complete_in_obj .

:complete_in_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :complete_in_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/buildingEndDate> ;
  lemon:subjOfProp  :complete_in_obj ;
  lemon:objOfProp   :complete_in_subj ;
  lemon:condition   :complete_in_sense_1_condition .

:complete_in_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Ship> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:complete_in_obj lemon:marker :in .

:arg2 lemon:marker :form_complete_in_present_preposition .

## Prepositions ##

:form_complete_in_present_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .