  lemon:canonicalForm  :form_entwickeln_1 ;
  lemon:otherForm      :form_entwickeln_1s ;
  lemon:otherForm      :form_entwickeln_1ed ;
  lemon:sense          :entwickeln_1_sense_1 ;
  lemon:synBehavior    :entwickeln_1_frame_transitive .

:form_entwickeln_1 a         lemon:Form ;
  lemon:writtenRep     "entwickeln"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_entwickeln_1s a    lemon:Form ;
  lemon:writtenRep "entwickelt"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .


:form_entwickeln_1s a    lemon:Form ;
  lemon:writtenRep "entwickelte"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .

:form_entwickeln_1ed a   lemon:Form ;
  lemon:writtenRep "entwickelt"@de ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:entwickeln_1_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :entwickeln_1_subj ;
  lexinfo:directObject     :entwickeln_1_obj .

:entwickeln_1_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :entwickeln_1_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp  :entwickeln_1_obj ;
  lemon:objOfProp   :entwickeln_1_subj ;
  lemon:condition   :entwickeln_1_sense_1_condition .


:entwickeln_1_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Software> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Company> .

:entwickeln_1ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_entwickeln_1ed_canonical ;
  lemon:otherForm      :form_entwickeln_1ed_by ;
