:entwickeln_1ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :entwickeln_1ed_subj ;
  lexinfo:prepositionalAdjunct :entwickeln_1ed_obj .

:entwickeln_1ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :entwickeln_1ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp  :entwickeln_1ed_subj ;
  lemon:objOfProp   :entwickeln_1ed_obj ;
