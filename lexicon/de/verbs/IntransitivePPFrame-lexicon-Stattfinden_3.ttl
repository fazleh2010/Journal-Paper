@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :Stattfinden_3 ;
  lemon:entry    :preposition .

:Stattfinden_3 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :Stattfinden_3_present ;
  lemon:otherForm      :Stattfinden_3_past ;
  lemon:otherForm      :Stattfinden_3_perfect ;
  lemon:sense          :Stattfinden_3_sense_1 ;
  lemon:synBehavior    :Stattfinden_3_frame .

:Stattfinden_3_present a           lemon:Form ;
  lemon:writtenRep     "stattfinden"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:Stattfinden_3_present a      lemon:Form ;
  lemon:writtenRep "findet statt"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:Stattfinden_3_past a lemon:Form ;
  lemon:writtenRep  "fand statt"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:Stattfinden_3_perfect a lemon:Form ;
  lemon:writtenRep  "stattgefunden"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:Stattfinden_3_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :Stattfinden_3_subj ;
  lexinfo:prepositionalAdjunct :Stattfinden_3_obj .

:Stattfinden_3_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :Stattfinden_3_ontomap ;
  lemon:reference   <http://dbpedia.org/property/date> ;
  lemon:subjOfProp  :Stattfinden_3_obj ;
  lemon:objOfProp   :Stattfinden_3_subj ;
  lemon:condition   :Stattfinden_3_sense_1_condition .

:Stattfinden_3_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/SocietalEvent> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:Stattfinden_3_obj lemon:marker :am .

## Prepositions ##
:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "am"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

