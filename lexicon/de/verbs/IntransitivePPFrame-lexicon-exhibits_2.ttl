@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :exhibits_2 ;
  lemon:entry    :form_exhibits_2_present_preposition .

:exhibits_2 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_exhibits_2_present ;
  lemon:otherForm      :exhibits_2_past ;
  lemon:otherForm      :exhibits_2_perfect ;
  lemon:sense          :exhibits_2_sense_1 ;
  lemon:synBehavior    :exhibits_2_frame .

:form_exhibits_2_presentexhibits_2_present a           lemon:Form ;
  lemon:writtenRep     "ausstellen"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:exhibits_2_present a      lemon:Form ;
  lemon:writtenRep "stellt aus"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:exhibits_2_past a lemon:Form ;
  lemon:writtenRep  "stellte aus"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:exhibits_2_perfect a lemon:Form ;
  lemon:writtenRep  "ausgestellt"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:exhibits_2_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :exhibits_2_subj ;
  lexinfo:prepositionalAdjunct :exhibits_2_obj .

:exhibits_2_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :exhibits_2_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/museum> ;
  lemon:subjOfProp  :exhibits_2_obj ;
  lemon:objOfProp   :exhibits_2_subj ;
  lemon:condition   :exhibits_2_sense_1_condition .

:exhibits_2_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Museum> .


:exhibits_2_obj lemon:marker :in .

:arg2 lemon:marker :form_exhibits_2_present_preposition .

## Prepositions ##

:form_exhibits_2_present_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .