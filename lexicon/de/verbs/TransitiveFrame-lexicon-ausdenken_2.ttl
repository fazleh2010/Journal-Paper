  lexinfo:person   lexinfo:thirdPerson .

:ausdenken_2_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :ausdenken_2_subj ;
  lexinfo:directObject     :ausdenken_2_obj .

:ausdenken_2_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :ausdenken_2_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp  :ausdenken_2_obj ;
  lemon:objOfProp   :ausdenken_2_subj ;
  lemon:condition   :ausdenken_2_sense_1_condition .


:ausdenken_2_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:ausdenken_2ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_ausdenken_2ed_canonical ;
