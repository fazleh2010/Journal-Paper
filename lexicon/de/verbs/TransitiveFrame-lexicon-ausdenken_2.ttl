  lemon:canonicalForm  :form_ausdenken_2 ;
  lemon:otherForm      :form_ausdenken_2s ;
  lemon:otherForm      :form_ausdenken_2ed ;
  lemon:sense          :ausdenken_2_sense_1 ;
  lemon:synBehavior    :ausdenken_2_frame_transitive .

:form_ausdenken_2 a         lemon:Form ;
  lemon:writtenRep     "ausdenken"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_ausdenken_2s a    lemon:Form ;
  lemon:writtenRep "dachte aus"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .


:form_ausdenken_2s a    lemon:Form ;
  lemon:writtenRep "denkt aus"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .

:form_ausdenken_2ed a   lemon:Form ;
  lemon:writtenRep "ausgedacht"@de ;
