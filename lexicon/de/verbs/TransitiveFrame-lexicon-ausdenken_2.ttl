  lexinfo:copulativeSubject    :ausdenken_2ed_subj ;
  lexinfo:prepositionalAdjunct :ausdenken_2ed_obj .

:ausdenken_2ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :ausdenken_2ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp  :ausdenken_2ed_subj ;
