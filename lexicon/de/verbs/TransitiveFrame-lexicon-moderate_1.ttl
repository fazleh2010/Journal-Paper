@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :to_moderate_1 ;
  lemon:entry    :moderate_1ed ;
  lemon:entry    :preposition .

:to_moderate_1 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_moderate_1 ;
  lemon:otherForm      :form_moderate_1s ;
  lemon:otherForm      :form_moderate_1ed ;
  lemon:sense          :moderate_1_sense_1 ;
  lemon:synBehavior    :moderate_1_frame_transitive .

:form_moderate_1 a         lemon:Form ;
  lemon:writtenRep     "moderieren"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_moderate_1s a    lemon:Form ;
  lemon:writtenRep "moderiert"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .


:form_moderate_1s a    lemon:Form ;
  lemon:writtenRep "moderiert"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .

:form_moderate_1ed a   lemon:Form ;
  lemon:writtenRep "hat moderiert"@de ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:moderate_1_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :moderate_1_subj ;
  lexinfo:directObject     :moderate_1_obj .

:moderate_1_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :moderate_1_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/presenter> ;
  lemon:subjOfProp  :moderate_1_obj ;
  lemon:objOfProp   :moderate_1_subj ;
  lemon:condition   :moderate_1_sense_1_condition .


:moderate_1_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:moderate_1ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_moderate_1ed_canonical ;
  lemon:otherForm      :form_moderate_1ed_by ;
  lemon:synBehavior    :moderate_1ed_frame_adjectivepp ;
  lemon:sense          :moderate_1ed_ontomap .

:form_moderate_1ed_canonical a lemon:Form ;
  lemon:writtenRep         "moderieren"@de .

:form_moderate_1ed_by a    lemon:Form ;
  lemon:writtenRep     "moderieren"@de ;
  lexinfo:verbFormMood lexinfo:participle .


:moderate_1ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :moderate_1ed_subj ;
  lexinfo:prepositionalAdjunct :moderate_1ed_obj .

:moderate_1ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :moderate_1ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/presenter> ;
  lemon:subjOfProp  :moderate_1ed_subj ;
  lemon:objOfProp   :moderate_1ed_obj ;
  lemon:condition   :moderate_1_sense_1_condition .
:moderate_1ed_obj lemon:marker :von .

## Prepositions ##
:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

