@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :nennen_1 ;
  lemon:entry    :preposition .

:nennen_1 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :nennen_1_present ;
  lemon:otherForm      :nennen_1_past ;
  lemon:otherForm      :nennen_1_perfect ;
  lemon:sense          :nennen_1_sense_1 ;
  lemon:synBehavior    :nennen_1_frame .

:nennen_1_present a           lemon:Form ;
  lemon:writtenRep     "nennen"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:nennen_1_present a      lemon:Form ;
  lemon:writtenRep "nennt"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:nennen_1_past a lemon:Form ;
  lemon:writtenRep  "nante"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:nennen_1_perfect a lemon:Form ;
  lemon:writtenRep  "genannt"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:nennen_1_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :nennen_1_subj ;
  lexinfo:prepositionalAdjunct :nennen_1_obj .

:nennen_1_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :nennen_1_ontomap ;
  lemon:reference   <http://dbpedia.org/property/nickname> ;
  lemon:subjOfProp  :nennen_1_obj ;
  lemon:objOfProp   :nennen_1_subj ;
  lemon:condition   :nennen_1_sense_1_condition .

:nennen_1_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .


:nennen_1_obj lemon:marker :X .

## Prepositions ##
:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "X"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

