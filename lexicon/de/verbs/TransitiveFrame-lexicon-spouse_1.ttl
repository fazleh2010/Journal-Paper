@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :to_spouse_1 ;
  lemon:entry    :spouse_1ed ;
  lemon:entry    :preposition .

:to_spouse_1 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_spouse_1 ;
  lemon:otherForm      :form_spouse_1s ;
  lemon:otherForm      :form_spouse_1ed ;
  lemon:sense          :spouse_1_sense_1 ;
  lemon:synBehavior    :spouse_1_frame_transitive .

:form_spouse_1 a         lemon:Form ;
  lemon:writtenRep     "verheiraten"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_spouse_1s a    lemon:Form ;
  lemon:writtenRep "verheiratet"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .


:form_spouse_1s a    lemon:Form ;
  lemon:writtenRep "verheiratete"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .

:form_spouse_1ed a   lemon:Form ;
  lemon:writtenRep "verheiratet"@de ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:spouse_1_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :spouse_1_subj ;
  lexinfo:directObject     :spouse_1_obj .

:spouse_1_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :spouse_1_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/spouse> ;
  lemon:subjOfProp  :spouse_1_obj ;
  lemon:objOfProp   :spouse_1_subj ;
  lemon:condition   :spouse_1_sense_1_condition .


:spouse_1_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:spouse_1ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_spouse_1ed_canonical ;
  lemon:otherForm      :form_spouse_1ed_by ;
  lemon:synBehavior    :spouse_1ed_frame_adjectivepp ;
  lemon:sense          :spouse_1ed_ontomap .

:form_spouse_1ed_canonical a lemon:Form ;
  lemon:writtenRep         "verheiraten"@de .

:form_spouse_1ed_by a    lemon:Form ;
  lemon:writtenRep     "verheiraten"@de ;
  lexinfo:verbFormMood lexinfo:participle .


:spouse_1ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :spouse_1ed_subj ;
  lexinfo:prepositionalAdjunct :spouse_1ed_obj .

:spouse_1ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :spouse_1ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/spouse> ;
  lemon:subjOfProp  :spouse_1ed_subj ;
  lemon:objOfProp   :spouse_1ed_obj ;
  lemon:condition   :spouse_1_sense_1_condition .
:spouse_1ed_obj lemon:marker :mit .

## Prepositions ##
:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "mit"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

