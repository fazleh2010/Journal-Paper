@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :storben_2 ;
  lemon:entry    :preposition .

:storben_2 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :storben_2_present ;
  lemon:otherForm      :storben_2_past ;
  lemon:otherForm      :storben_2_perfect ;
  lemon:sense          :storben_2_sense_1 ;
  lemon:synBehavior    :storben_2_frame .

:storben_2_present a           lemon:Form ;
  lemon:writtenRep     "sterben"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:storben_2_present a      lemon:Form ;
  lemon:writtenRep "stirbt"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:storben_2_past a lemon:Form ;
  lemon:writtenRep  "starb"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:storben_2_perfect a lemon:Form ;
  lemon:writtenRep  "gestorben"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:storben_2_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :storben_2_subj ;
  lexinfo:prepositionalAdjunct :storben_2_obj .

:storben_2_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :storben_2_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/deathCause> ;
  lemon:subjOfProp  :storben_2_obj ;
  lemon:objOfProp   :storben_2_subj ;
  lemon:condition   :storben_2_sense_1_condition .

:storben_2_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .


:storben_2_obj lemon:marker :in .

## Prepositions ##
:preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

