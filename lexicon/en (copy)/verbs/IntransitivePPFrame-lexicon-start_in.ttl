@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :start_in ;
  lemon:entry    :form_start_in_preposition .

:start_in a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_start_in ;
  lemon:otherForm      :form_start_in_past ;
  lemon:sense          :start_in_sense_1 ;
  lemon:synBehavior    :start_in_frame .

:form_start_in a           lemon:Form ;
  lemon:writtenRep     "start"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_start_in a      lemon:Form ;
  lemon:writtenRep "starts"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_start_in_past a lemon:Form ;
  lemon:writtenRep  "started"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:start_in_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :start_in_subj ;
  lexinfo:prepositionalAdjunct :start_in_obj .

:start_in_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :start_in_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/sourceCountry> ;
  lemon:subjOfProp  :start_in_obj ;
  lemon:objOfProp   :start_in_subj ;
  lemon:condition   :start_in_sense_1_condition .

:start_in_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:start_in_obj lemon:marker :in .

## Prepositions ##
:form_start_in_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

