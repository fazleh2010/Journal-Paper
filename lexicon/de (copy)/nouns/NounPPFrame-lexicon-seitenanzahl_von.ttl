@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :seitenanzahl_von ;
  lemon:entry    :seitenanzahl_von_form_preposition .

:seitenanzahl_von a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :seitenanzahl_von_form ;
  lemon:otherForm      :seitenanzahl_von_plural_form ;
  lemon:otherForm      :seitenanzahl_von_accusative_form ;
  lemon:otherForm      :seitenanzahl_von_dative_form ;
  lemon:otherForm      :seitenanzahl_von_genitive_form ;
  lemon:otherForm      :seitenanzahl_von_plural_form ;
  lemon:sense          :seitenanzahl_von_sense_1 ;
  lemon:synBehavior    :seitenanzahl_von_nounpp .

:seitenanzahl_von_form a lemon:Form ;
  lemon:writtenRep "Seitenzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:nominativeCase .

:seitenanzahl_von_plural_form a lemon:Form ;
  lemon:writtenRep "Seitenzahlen"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:plural ;
  lexinfo:case     lexinfo:nominativeCase .

:seitenanzahl_von_accusative_form a lemon:Form ;
  lemon:writtenRep "Seitenzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:accusativeCase .

:seitenanzahl_von_dative_form a lemon:Form ;
  lemon:writtenRep "Seitenzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:dativeCase .

:seitenanzahl_von_genitive_form a lemon:Form ;
  lemon:writtenRep "Seitenzahl"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:genitiveCase .

:seitenanzahl_von_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:seitenanzahl_von_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :seitenanzahl_von_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfPages> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :seitenanzahl_von_sense_1_condition .

:seitenanzahl_von_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/WrittenWork> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#positiveInteger> .

:arg2 lemon:marker :seitenanzahl_von_form_preposition .

## Prepositions ##

:seitenanzahl_von_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .