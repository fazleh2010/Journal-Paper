@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :participate_in_2 ;
  lemon:entry    :form_participate_in_2_preposition .

:participate_in_2 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_participate_in_2 ;
  lemon:otherForm      :form_participate_in_2_past ;
  lemon:sense          :participate_in_2_sense_1 ;
  lemon:synBehavior    :participate_in_2_frame .

:form_participate_in_2 a           lemon:Form ;
  lemon:writtenRep     "participate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_participate_in_2 a      lemon:Form ;
  lemon:writtenRep "participates"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_participate_in_2_past a lemon:Form ;
  lemon:writtenRep  "participated"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_participate_in_2perfect a   lemon:Form ;
  lemon:writtenRep "participated"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:participate_in_2_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :participate_in_2_subj ;
  lexinfo:prepositionalAdjunct :participate_in_2_obj .

:participate_in_2_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :participate_in_2_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/battle> ;
  lemon:subjOfProp  :participate_in_2_obj ;
  lemon:objOfProp   :participate_in_2_subj ;
  lemon:condition   :participate_in_2_sense_1_condition .

:participate_in_2_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MilitaryPerson> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/MilitaryConflict> .


:participate_in_2_obj lemon:marker :form_participate_in_2_preposition .

## Prepositions ##
:form_participate_in_2_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

