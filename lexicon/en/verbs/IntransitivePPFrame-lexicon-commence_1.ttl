@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :commence_1 ;
  lemon:entry    :form_commence_1_preposition .

:commence_1 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_commence_1 ;
  lemon:otherForm      :form_commence_1_past ;
  lemon:sense          :commence_1_sense_1 ;
  lemon:synBehavior    :commence_1_frame .

:form_commence_1 a           lemon:Form ;
  lemon:writtenRep     "commence"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_commence_1 a      lemon:Form ;
  lemon:writtenRep "commences"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_commence_1_past a lemon:Form ;
  lemon:writtenRep  "commenced"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:commence_1_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :commence_1_subj ;
  lexinfo:prepositionalAdjunct :commence_1_obj .

:commence_1_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :commence_1_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/date> ;
  lemon:subjOfProp  :commence_1_obj ;
  lemon:objOfProp   :commence_1_subj ;
  lemon:condition   :commence_1_sense_1_condition .

:commence_1_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Event> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:commence_1_obj lemon:marker :in .

## Prepositions ##
:form_commence_1_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

