@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :reopen_on_3 ;
  lemon:entry    :form_reopen_on_3_preposition .

:reopen_on_3 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_reopen_on_3 ;
  lemon:otherForm      :form_reopen_on_3_past ;
  lemon:sense          :reopen_on_3_sense_1 ;
  lemon:synBehavior    :reopen_on_3_frame .

:form_reopen_on_3 a           lemon:Form ;
  lemon:writtenRep     "reopen"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_reopen_on_3 a      lemon:Form ;
  lemon:writtenRep "reopens"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_reopen_on_3_past a lemon:Form ;
  lemon:writtenRep  "reopened"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:reopen_on_3_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :reopen_on_3_subj ;
  lexinfo:prepositionalAdjunct :reopen_on_3_obj .

:reopen_on_3_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :reopen_on_3_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/reopeningDate> ;
  lemon:subjOfProp  :reopen_on_3_obj ;
  lemon:objOfProp   :reopen_on_3_subj ;
  lemon:condition   :reopen_on_3_sense_1_condition .

:reopen_on_3_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:reopen_on_3_obj lemon:marker :on .

## Prepositions ##
:form_reopen_on_3_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

