@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :founded_in_2 ;
  lemon:entry    :form_founded_in_2_preposition .

:founded_in_2 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_founded_in_2 ;
  lemon:otherForm      :form_founded_in_2_past ;
  lemon:sense          :founded_in_2_sense_1 ;
  lemon:synBehavior    :founded_in_2_frame .

:form_founded_in_2 a           lemon:Form ;
  lemon:writtenRep     "found"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_founded_in_2 a      lemon:Form ;
  lemon:writtenRep "founds"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_founded_in_2_past a lemon:Form ;
  lemon:writtenRep  "founded"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:founded_in_2_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :founded_in_2_subj ;
  lexinfo:prepositionalAdjunct :founded_in_2_obj .

:founded_in_2_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :founded_in_2_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/foundingDate> ;
  lemon:subjOfProp  :founded_in_2_obj ;
  lemon:objOfProp   :founded_in_2_subj ;
  lemon:condition   :founded_in_2_sense_1_condition .

:founded_in_2_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Settlement> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#gYear> .


:founded_in_2_obj lemon:marker :in .

## Prepositions ##
:form_founded_in_2_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

