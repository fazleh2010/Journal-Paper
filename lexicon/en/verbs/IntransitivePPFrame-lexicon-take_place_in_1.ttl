@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :take_place_in_1 ;
  lemon:entry    :form_take_place_in_1_preposition .

:take_place_in_1 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_take_place_in_1 ;
  lemon:otherForm      :form_take_place_in_1_past ;
  lemon:sense          :take_place_in_1_sense_1 ;
  lemon:synBehavior    :take_place_in_1_frame .

:form_take_place_in_1 a           lemon:Form ;
  lemon:writtenRep     "take place"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_take_place_in_1 a      lemon:Form ;
  lemon:writtenRep "takes place"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_take_place_in_1_past a lemon:Form ;
  lemon:writtenRep  "took place"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:take_place_in_1_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :take_place_in_1_subj ;
  lexinfo:prepositionalAdjunct :take_place_in_1_obj .

:take_place_in_1_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :take_place_in_1_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/place> ;
  lemon:subjOfProp  :take_place_in_1_obj ;
  lemon:objOfProp   :take_place_in_1_subj ;
  lemon:condition   :take_place_in_1_sense_1_condition .

:take_place_in_1_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MilitaryConflict> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .


:take_place_in_1_obj lemon:marker :in .

## Prepositions ##
:form_take_place_in_1_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

