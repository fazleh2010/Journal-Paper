@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :pages_of ;
  lemon:entry    :pages_of_form_preposition .

:pages_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :pages_of_form ;
  lemon:otherForm  :pages_of_singular ;
  lemon:otherForm  :pages_of_plural ;
  lemon:sense          :pages_of_sense_1 ;
  lemon:synBehavior    :pages_of_nounpp .

:pages_of_form a lemon:Form ;
  lemon:writtenRep "pages"@en .

:pages_of_singular a    lemon:Form ;
  lemon:writtenRep "pages"@en ;
  lexinfo:number   lexinfo:singular .

:pages_of_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:pages_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:pages_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :pages_of_sense_1 ;
  lemon:reference           <http://swrc.ontoware.org/ontology#pages> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :pages_of_sense_1_condition .

:pages_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://swrc.ontoware.org/ontology#Publication> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :pages_of_form_preposition .
## Prepositions ##
:pages_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

