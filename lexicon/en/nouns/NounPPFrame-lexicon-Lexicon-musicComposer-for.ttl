@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Lexicon-musicComposer-for ;
  lemon:entry    :for .

:Lexicon-musicComposer-for a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Lexicon-musicComposer-for_form ;
  lemon:sense          :Lexicon-musicComposer-for_sense_1 ;
  lemon:synBehavior    :Lexicon-musicComposer-for_nounpp .

:Lexicon-musicComposer-for_form a lemon:Form ;
  lemon:writtenRep "music composer"@en .

:Lexicon-musicComposer-for_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Lexicon-musicComposer-for_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Lexicon-musicComposer-for_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/musicComposer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Lexicon-musicComposer-for_sense_1_condition .

:Lexicon-musicComposer-for_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/MusicalArtist> .

:arg2 lemon:marker :for .

## Prepositions ##

:for a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "for"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .