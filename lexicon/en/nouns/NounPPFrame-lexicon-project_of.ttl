@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :project_of ;
  lemon:entry    :project_of_form_preposition .

:project_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :project_of_form ;
  lemon:otherForm  :project_of_singular ;
  lemon:otherForm  :project_of_plural ;
  lemon:sense          :project_of_sense_1 ;
  lemon:synBehavior    :project_of_nounpp .

:project_of_form a lemon:Form ;
  lemon:writtenRep "project "@en .

:project_of_singular a    lemon:Form ;
  lemon:writtenRep "project "@en ;
  lexinfo:number   lexinfo:singular .

:project_of_plural a   lemon:Form ;
  lemon:writtenRep "projects"@en ;
  lexinfo:number    lexinfo:plural .

:project_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:project_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :project_of_sense_1 ;
  lemon:reference           <http://swrc.ontoware.org/ontology#project> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :project_of_sense_1_condition .

:project_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://swrc.ontoware.org/ontology#Publication> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :project_of_form_preposition .
## Prepositions ##
:project_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

