@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :timeZone-of_1 ;
  lemon:entry    :timeZone-of_1_form_preposition .

:timeZone-of_1 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :timeZone-of_1_form ;
  lemon:otherForm  :timeZone-of_1_singular ;
  lemon:otherForm  :timeZone-of_1_plural ;
  lemon:sense          :timeZone-of_1_sense_1 ;
  lemon:synBehavior    :timeZone-of_1_nounpp .

:timeZone-of_1_form a lemon:Form ;
  lemon:writtenRep "time zone"@en .

:timeZone-of_1_singular a    lemon:Form ;
  lemon:writtenRep "time zone"@en ;
  lexinfo:number   lexinfo:singular .

:timeZone-of_1_plural a   lemon:Form ;
  lemon:writtenRep "time zones"@en ;
  lexinfo:number    lexinfo:plural .

:timeZone-of_1_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:timeZone-of_1_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :timeZone-of_1_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/timeZone> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :timeZone-of_1_sense_1_condition .

:timeZone-of_1_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :timeZone-of_1_form_preposition .
## Prepositions ##
:timeZone-of_1_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

