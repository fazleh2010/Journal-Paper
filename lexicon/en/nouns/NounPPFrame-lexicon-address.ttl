@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :address ;
  lemon:entry    :address_form_preposition .

:address a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :address_form ;
  lemon:otherForm  :address_singular ;
  lemon:otherForm  :address_plural ;
  lemon:sense          :address_sense_1 ;
  lemon:synBehavior    :address_nounpp .

:address_form a lemon:Form ;
  lemon:writtenRep "address"@en .

:address_singular a    lemon:Form ;
  lemon:writtenRep "address"@en ;
  lexinfo:number   lexinfo:singular .

:address_plural a   lemon:Form ;
  lemon:writtenRep "addresses"@en ;
  lexinfo:number    lexinfo:plural .

:address_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:address_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :address_sense_1 ;
  lemon:reference           <http://swrc.ontoware.org/ontology#address> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :address_sense_1_condition .

:address_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://swrc.ontoware.org/ontology#Publication> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :address_form_preposition .
## Prepositions ##
:address_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

