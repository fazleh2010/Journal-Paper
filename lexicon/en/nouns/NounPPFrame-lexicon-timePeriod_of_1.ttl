@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :timePeriod_of_1 ;
  lemon:entry    :of .

:timePeriod_of_1 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :timePeriod_of_1_form ;
  lemon:sense          :timePeriod_of_1_sense_1 ;
  lemon:synBehavior    :timePeriod_of_1_nounpp .

:timePeriod_of_1_form a lemon:Form ;
  lemon:writtenRep "time period"@en .

:timePeriod_of_1_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:timePeriod_of_1_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :timePeriod_of_1_sense_1 ;
  lemon:reference           <http%3A%2F%2Fwww.citizensinformation.ie%2F%23hasPeriod> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :timePeriod_of_1_sense_1_condition .

:timePeriod_of_1_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http%3A%2F%2Fwww.citizensinformation.ie%2F%23Income> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

## Prepositions ##
:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

