@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base   <http://localhost:8080#> .

:lexicon_en
    a              lemon:Lexicon ;
    lemon:language "en" ;
    lemon:entry    :deep ;
    lemon:entry    :deep_res .

:deep
    a                    lemon:LexicalEntry ;
    lexinfo:partOfSpeech lexinfo:adjective ;
    lemon:canonicalForm  :deep_lemma ;
    lemon:synBehavior    :deep_attrFrame, :deep_predFrame ;
    lemon:sense          :deep_sense .

:deep_lemma
    lemon:writtenRep "deep"@en .

:deep_predFrame
    a                         lexinfo:AdjectivePredicateFrame ;
    lexinfo:copulativeSubject :deep_PredSynArg .

:deep_attrFrame
    a                      lexinfo:AdjectiveAttributiveFrame ;
    lexinfo:attributiveArg :deep_AttrSynArg .

:deep_sense
    a               lemon:LexicalSense ;
    lemon:reference :deep_res ;
    lemon:isA       :deep_AttrSynArg, :deep_PredSynArg .

:deep_res
    a              owl:Restriction ;
    owl:onProperty <http://dbpedia.org/ontology/maximumDepth>;
    owl:hasValue   <http://dbpedia.org/resource/Reservoir> .
