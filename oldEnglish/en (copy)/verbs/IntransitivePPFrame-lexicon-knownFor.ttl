@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :knownFor ;
  lemon:entry    :form_knownFor_preposition .

:knownFor a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_knownFor ;
  lemon:otherForm      :form_knownFor_past ;
  lemon:sense          :knownFor_sense_1 ;
  lemon:synBehavior    :knownFor_frame .

:form_knownFor a           lemon:Form ;
  lemon:writtenRep     "known"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_knownFor a      lemon:Form ;
  lemon:writtenRep "known"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_knownFor_past a lemon:Form ;
  lemon:writtenRep  "known"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:knownFor_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :knownFor_subj ;
  lexinfo:prepositionalAdjunct :knownFor_obj .

:knownFor_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :knownFor_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/knownFor> ;
  lemon:subjOfProp  :knownFor_obj ;
  lemon:objOfProp   :knownFor_subj ;
  lemon:condition   :knownFor_sense_1_condition .

:knownFor_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Award> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:knownFor_obj lemon:marker :for .

## Prepositions ##
:form_knownFor_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "for"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

