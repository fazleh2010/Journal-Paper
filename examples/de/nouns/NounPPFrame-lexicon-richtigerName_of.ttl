@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :richtigerName_of ;
  lemon:entry    :richtigerName_of_form_preposition .

:richtigerName_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :richtigerName_of_form ;
  lemon:otherForm      :richtigerName_of_plural_form ;
  lemon:otherForm      :richtigerName_of_accusative_form ;
  lemon:otherForm      :richtigerName_of_dative_form ;
  lemon:otherForm      :richtigerName_of_genitive_form ;
  lemon:otherForm      :richtigerName_of_plural_form ;
  lemon:sense          :richtigerName_of_sense_1 ;
  lemon:synBehavior    :richtigerName_of_nounpp .

:richtigerName_of_form a lemon:Form ;
  lemon:writtenRep "richtiger Name"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:nominativeCase .

:richtigerName_of_plural_form a lemon:Form ;
  lemon:writtenRep "richtiger Name"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:plural ;
  lexinfo:case     lexinfo:nominativeCase .

:richtigerName_of_accusative_form a lemon:Form ;
  lemon:writtenRep "richtiger Name"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:accusativeCase .

:richtigerName_of_dative_form a lemon:Form ;
  lemon:writtenRep "range"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:dativeCase .

:richtigerName_of_genitive_form a lemon:Form ;
  lemon:writtenRep "domain"@de ;
  lexinfo:gender   lexinfo:feminine ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:genitiveCase .

:richtigerName_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:richtigerName_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :richtigerName_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/alterEgo> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :richtigerName_of_sense_1_condition .

:richtigerName_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/FictionalCharacter> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :richtigerName_of_form_preposition .

## Prepositions ##

:richtigerName_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .