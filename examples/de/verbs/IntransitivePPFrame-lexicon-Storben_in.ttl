@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :Storben_in ;
  lemon:entry    :form_Storben_in_present_preposition .

:Storben_in a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_Storben_in_present ;
  lemon:otherForm      :Storben_in_present3rd ;
  lemon:otherForm      :Storben_in_past ;
  lemon:otherForm      :Storben_in_perfect ;
  lemon:sense          :Storben_in_sense_1 ;
  lemon:synBehavior    :Storben_in_frame .

:form_Storben_in_present a           lemon:Form ;
  lemon:writtenRep     "sterben"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .


:Storben_in_present3rd a      lemon:Form ;
  lemon:writtenRep "stirbt"@de ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:Storben_in_past a lemon:Form ;
  lemon:writtenRep  "starb"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:Storben_in_perfect a lemon:Form ;
  lemon:writtenRep  "gestorben"@de ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:Storben_in_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :Storben_in_subj ;
  lexinfo:prepositionalAdjunct :Storben_in_obj .

:Storben_in_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :Storben_in_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/deathPlace> ;
  lemon:subjOfProp  :Storben_in_obj ;
  lemon:objOfProp   :Storben_in_subj ;
  lemon:condition   :Storben_in_sense_1_condition .

:Storben_in_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/City> .


:Storben_in_obj lemon:marker :in .

:arg2 lemon:marker :form_Storben_in_present_preposition .

## Prepositions ##

:form_Storben_in_present_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .