@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "it" ;
  lemon:entry    :born_in_person ;
  lemon:entry    :form_born_in_person_present_preposition .

:born_in_person a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_born_in_person_present ;
  lemon:otherForm      :born_in_person_present3rd ;
  lemon:otherForm      :born_in_person_past ;
  lemon:otherForm      :born_in_person_perfect ;
  lemon:sense          :born_in_person_sense_1 ;
  lemon:synBehavior    :born_in_person_frame .

:form_born_in_person_present a           lemon:Form ;
  lemon:writtenRep     "nascere"@it ;
  lexinfo:verbFormMood lexinfo:infinitive .


:born_in_person_present3rd a      lemon:Form ;
  lemon:writtenRep "nasce"@it ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:born_in_person_past a lemon:Form ;
  lemon:writtenRep  "� nata"@it ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:born_in_person_perfect a lemon:Form ;
  lemon:writtenRep  "� nata"@it ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:perfect .

:born_in_person_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :born_in_person_subj ;
  lexinfo:prepositionalAdjunct :born_in_person_obj .

:born_in_person_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :born_in_person_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp  :born_in_person_obj ;
  lemon:objOfProp   :born_in_person_subj ;
  lemon:condition   :born_in_person_sense_1_condition .

:born_in_person_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Place> .


:born_in_person_obj lemon:marker :in .

:arg2 lemon:marker :form_born_in_person_present_preposition .

## Prepositions ##

:form_born_in_person_present_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .