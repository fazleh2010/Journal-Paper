@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :TimeZone-of_2 ;
  lemon:entry    :TimeZone-of_2_form_preposition .

:TimeZone-of_2 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :TimeZone-of_2_form ;
  lemon:otherForm  :TimeZone-of_2_singular ;
  lemon:otherForm  :TimeZone-of_2_plural ;
  lemon:sense          :TimeZone-of_2_sense_1 ;
  lemon:synBehavior    :TimeZone-of_2_nounpp .

:TimeZone-of_2_form a lemon:Form ;
  lemon:writtenRep "time zone"@en .

:TimeZone-of_2_singular a    lemon:Form ;
  lemon:writtenRep "time zone"@en ;
  lexinfo:number   lexinfo:singular .

:TimeZone-of_2_plural a   lemon:Form ;
  lemon:writtenRep "time zones"@en ;
  lexinfo:number    lexinfo:plural .

:TimeZone-of_2_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:TimeZone-of_2_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :TimeZone-of_2_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/timeZone> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :TimeZone-of_2_sense_1_condition .

:TimeZone-of_2_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Town> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

## Prepositions ##
:TimeZone-of_2_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

