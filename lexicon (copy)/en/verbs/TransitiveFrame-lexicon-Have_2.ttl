@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_Have_2 ;
  lemon:entry    :Have_2ed ;
  lemon:entry    :form_Have_2_preposition .

:to_Have_2 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_Have_2 ;
  lemon:otherForm      :form_Have_2s ;
  lemon:otherForm      :form_Have_2ed ;
  lemon:otherForm      :form_Have_2perfect ;
  lemon:sense          :Have_2_sense_1 ;
  lemon:synBehavior    :Have_2_frame_transitive .

:form_Have_2 a         lemon:Form ;
  lemon:writtenRep     "have"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_Have_2s a    lemon:Form ;
  lemon:writtenRep "has"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_Have_2ed a   lemon:Form ;
  lemon:writtenRep "had"@en ;
  lexinfo:tense    lexinfo:past .

:form_Have_2perfect a   lemon:Form ;
  lemon:writtenRep "had"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:Have_2_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :Have_2_subj ;
  lexinfo:directObject     :Have_2_obj .

:Have_2_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :Have_2_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/numberOfLocations> ;
  lemon:subjOfProp  :Have_2_obj ;
  lemon:objOfProp   :Have_2_subj ;
  lemon:condition   :Have_2_sense_1_condition .


:Have_2_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:Have_2ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_Have_2ed_canonical ;
  lemon:otherForm      :form_Have_2ed_by ;
  lemon:synBehavior    :Have_2ed_frame_adjectivepp ;
  lemon:sense          :Have_2ed_ontomap .

:form_Have_2ed_canonical a lemon:Form ;
  lemon:writtenRep         "have"@en .

:form_Have_2ed_by a    lemon:Form ;
  lemon:writtenRep     "have"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:Have_2ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :Have_2ed_subj ;
  lexinfo:prepositionalAdjunct :Have_2ed_obj .

:Have_2ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :Have_2ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/numberOfLocations> ;
  lemon:subjOfProp  :Have_2ed_subj ;
  lemon:objOfProp   :Have_2ed_obj ;
  lemon:condition   :Have_2_sense_1_condition .
:Have_2ed_obj lemon:marker :form_Have_2_preposition .

## Prepositions ##
:form_Have_2_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

