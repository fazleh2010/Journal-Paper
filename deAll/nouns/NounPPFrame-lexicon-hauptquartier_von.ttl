@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :hauptquartier_von ;
  lemon:entry    :hauptquartier_von_form_preposition .

:hauptquartier_von a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :hauptquartier_von_form ;
  lemon:otherForm      :hauptquartier_von_plural_form ;
  lemon:otherForm      :hauptquartier_von_accusative_form ;
  lemon:otherForm      :hauptquartier_von_dative_form ;
  lemon:otherForm      :hauptquartier_von_genitive_form ;
  lemon:otherForm      :hauptquartier_von_plural_form ;
  lemon:sense          :hauptquartier_von_sense_1 ;
  lemon:synBehavior    :hauptquartier_von_nounpp .

:hauptquartier_von_form a lemon:Form ;
  lemon:writtenRep "Hauptquartier"@de ;
  lexinfo:gender   lexinfo:neuter ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:nominativeCase .

:hauptquartier_von_plural_form a lemon:Form ;
  lemon:writtenRep "Hauptquaritere"@de ;
  lexinfo:gender   lexinfo:neuter ;
  lexinfo:number   lexinfo:plural ;
  lexinfo:case     lexinfo:nominativeCase .

:hauptquartier_von_accusative_form a lemon:Form ;
  lemon:writtenRep "Hauptquartier"@de ;
  lexinfo:gender   lexinfo:neuter ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:accusativeCase .

:hauptquartier_von_dative_form a lemon:Form ;
  lemon:writtenRep "Hauptquartier"@de ;
  lexinfo:gender   lexinfo:neuter ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:dativeCase .

:hauptquartier_von_genitive_form a lemon:Form ;
  lemon:writtenRep "Hauptquartiers"@de ;
  lexinfo:gender   lexinfo:neuter ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:case     lexinfo:genitiveCase .

:hauptquartier_von_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:hauptquartier_von_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :hauptquartier_von_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :hauptquartier_von_sense_1_condition .

:hauptquartier_von_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2 lemon:marker :hauptquartier_von_form_preposition .

## Prepositions ##

:hauptquartier_von_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .