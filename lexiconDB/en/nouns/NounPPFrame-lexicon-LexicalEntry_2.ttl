@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :LexicalEntry_2 ;
  lemon:entry    :of .

:LexicalEntry_2 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :LexicalEntry_2_form ;
  lemon:synBehavior    :LexicalEntry_2_nounpp ;
  lemon:sense          :LexicalEntry_2_sense_ontomap .

:LexicalEntry_2_form a lemon:Form ;
  lemon:writtenRep "birth place"@en .

:LexicalEntry_2_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:LexicalEntry_2_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :LexicalEntry_2_sense_ontomap ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :LexicalEntry_2_condition .

:LexicalEntry_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .