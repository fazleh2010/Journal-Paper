@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "it" ;
  lemon:entry    :end_in ;
  lemon:entry    :nel .

:end_in a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_end_in ;
  lemon:otherForm      :form_end_in_past ;
  lemon:sense          :end_in_sense_1 ;
  lemon:synBehavior    :end_in_frame .

:form_end_in a           lemon:Form ;
  lemon:writtenRep     "finire"@it ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_end_in a      lemon:Form ;
  lemon:writtenRep "finisce"@it ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_end_in_past a lemon:Form ;
  lemon:writtenRep  "finito"@it ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:end_in_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :end_in_subj ;
  lexinfo:prepositionalAdjunct :end_in_obj .

:end_in_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :end_in_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/mouthPlace> ;
  lemon:subjOfProp  :end_in_obj ;
  lemon:objOfProp   :end_in_subj ;
  lemon:condition   :end_in_sense_1_condition .

:end_in_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .


:end_in_obj lemon:marker :nel .

## Prepositions ##
:nel a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "nel"@it ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

