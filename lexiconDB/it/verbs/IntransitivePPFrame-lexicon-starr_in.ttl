@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "it" ;
  lemon:entry    :starr_in ;
  lemon:entry    :in .

:starr_in a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_starr_in ;
  lemon:otherForm      :form_starr_in_past ;
  lemon:sense          :starr_in_sense_1 ;
  lemon:synBehavior    :starr_in_frame .

:form_starr_in a           lemon:Form ;
  lemon:writtenRep     "recitare"@it ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_starr_in a      lemon:Form ;
  lemon:writtenRep "recita"@it ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_starr_in_past a lemon:Form ;
  lemon:writtenRep  "ha recitato"@it ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:starr_in_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :starr_in_subj ;
  lexinfo:prepositionalAdjunct :starr_in_obj .

:starr_in_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :starr_in_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/starring> ;
  lemon:subjOfProp  :starr_in_obj ;
  lemon:objOfProp   :starr_in_subj ;
  lemon:condition   :starr_in_sense_1_condition .

:starr_in_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Actor> .


:starr_in_obj lemon:marker :in .

## Prepositions ##
:in a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@it ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

