@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "it" ;
  lemon:entry    :founded_on ;
  lemon:entry    :nel .

:founded_on a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_founded_on ;
  lemon:otherForm      :form_founded_on_past ;
  lemon:sense          :founded_on_sense_1 ;
  lemon:synBehavior    :founded_on_frame .

:form_founded_on a           lemon:Form ;
  lemon:writtenRep     "fondare"@it ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_founded_on a      lemon:Form ;
  lemon:writtenRep "fonda"@it ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_founded_on_past a lemon:Form ;
  lemon:writtenRep  "fondato"@it ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .

:founded_on_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :founded_on_subj ;
  lexinfo:prepositionalAdjunct :founded_on_obj .

:founded_on_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :founded_on_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/foundingDate> ;
  lemon:subjOfProp  :founded_on_obj ;
  lemon:objOfProp   :founded_on_subj ;
  lemon:condition   :founded_on_sense_1_condition .

:founded_on_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema/date> .


:founded_on_obj lemon:marker :nel .

## Prepositions ##
:nel a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "nel"@it ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

